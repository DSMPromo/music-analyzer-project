{
  "name": "music-analyzer",
  "version": "1.0.0",
  "description": "AI-powered music analyzer and MIDI generator with spectrum visualization, chord detection, and Logic Pro integration",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && PORT=56400 npm start",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "setup": "npm install && cd client && npm install && pip install basic-pitch --break-system-packages"
  },
  "keywords": [
    "music",
    "audio",
    "midi",
    "spectrum-analyzer",
    "chord-detection",
    "basic-pitch",
    "web-audio-api",
    "react",
    "logic-pro"
  ],
  "author": "DSM.Promo",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "ytdlp-nodejs": "^2.3.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dsm-promo/music-analyzer"
  }
}
